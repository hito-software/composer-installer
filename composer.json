{
    "name": "hito-software/composer-installer",
    "description": ":package_description",
    "type": "composer-plugin",
    "keywords": [],
    "homepage": "https://github.com/hito-software/composer-installer",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "composer-plugin-api": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Hito\\Installer\\": "src/"
        }
    },
    "extra": {
        "class": "Hito\\Installer\\Plugin",
        "plugin-modifies-install-path": true
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
